{
  "private": true,
  "name": "clayui.com",
  "version": "3.0.0",
  "license": "MIT",
  "scripts": {
    "copy:clay-css": "yarn copy:clay-css-css && yarn copy:clay-css-icons && yarn copy:clay-css-js && yarn copy:clay-css-site-images",
    "copy:clay-css-css": "cpx '../node_modules/clay-css/lib/css/*.{css,css.map}' static/css",
    "copy:clay-css-icons": "cpx '../node_modules/clay-css/lib/images/icons/*.svg' static/images/icons",
    "copy:clay-css-js": "cpx '../node_modules/clay-css/lib/js/*.{js,js.map}' static/js",
    "copy:clay-css-site-images": "cpx '../node_modules/clay-css/src/content/site-images/*.svg' static/images",
    "deploy": "gatsby build && gh-pages -d public -b wedeploy",
    "develop": "yarn copy:clay-css && gatsby develop",
    "build": "yarn copy:clay-css && gatsby build",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "@clayui/button": "^3.0.0",
    "@clayui/checkbox": "^3.0.0",
    "@clayui/drop-down": "^3.0.0",
    "@clayui/date-picker": "^3.0.0",
    "@clayui/icon": "^3.0.0",
    "@clayui/select": "^3.0.0",
    "@clayui/navigation-bar": "^3.0.0",
    "@clayui/link": "^3.0.0",
    "@mdx-js/mdx": "^0.16.8",
    "@mdx-js/tag": "^0.16.8",
    "axios": "^0.18.0",
    "browser-monads": "^1.0.0",
    "classnames": "^2.2.6",
    "clay-component": "^2.7.0",
    "clay-css": "^2.8.0",
    "clay-tooltip": "^2.6.0",
    "countries-list": "^2.3.2",
    "dotenv": "^6.0.0",
    "gatsby": "^2.3.25",
    "gatsby-mdx": "^0.3.6-ci.297",
    "gatsby-plugin-google-analytics": "^2.0.6",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-sass": "^2.0.10",
    "gatsby-plugin-typescript": "^2.0.13",
    "gatsby-remark-prismjs": "^3.2.8",
    "gatsby-source-filesystem": "^2.0.20",
    "gatsby-transformer-remark": "^2.2.4",
    "js-yaml": "^3.12.1",
    "languages": "^0.1.3",
    "metal-clipboard": "^2.0.0",
    "metal-dom": "^2.16.5",
    "metal-tabs": "^2.3.1",
    "node-sass": "^4.11.0",
    "prismjs": "^1.15.0",
    "react": "^16.8.1",
    "react-docgen": "5.0.0-beta.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-live": "^2.0.1"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "gh-pages": "^1.2.0",
    "globby": "^8.0.1"
  }
}
