{
	"browserslist": [
		"extends browserslist-config-clay"
	],
	"dependencies": {
		"billboard.js": "^1.8.0",
		"classnames": "2.3.1",
		"d3": "^5.9.2"
	},
	"description": "React.js wrapper for D3 and billboard.js",
	"files": [
		"lib"
	],
	"keywords": [
		"clay",
		"react"
	],
	"main": "src/index.tsx",
	"name": "@clayui/charts",
	"peerDependencies": {
		"@clayui/css": "^3.159.0",
		"react": "^16.0.0 || ^17.0.0 || ^18.0.0",
		"react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0"
	},
	"scripts": {
		"build": "yarn build:cjs && yarn build:esm",
		"build:cjs": "cross-env NODE_ENV=production node ../build-package-esbuild.js --format=cjs --outdir=lib/cjs",
		"build:esm": "cross-env NODE_ENV=production node ../build-package-esbuild.js --format=esm --outdir=lib/esm",
		"buildTypes": "cross-env NODE_ENV=production tsc --project ./tsconfig.declarations.json",
		"copySvg": "ncp src/svg lib/svg",
		"prepublishOnly": "yarn sass && yarn build && yarn buildTypes && yarn copySvg",
		"sass": "sass src/scss/main.scss lib/css/main.css --source-map",
		"test": "jest --config ../../jest.config.js"
	},
	"version": "3.156.0"
}
