{
  "name": "lexicon",
  "main": "release/scss/atlas.scss",
  "homepage": "https://liferay.github.io/lexicon",
  "authors": [
    "Nate Cavanaugh",
    "Patrick Yeo"
  ],
  "license": "MIT",
  "ignore": [
    "**/*",
    "!release/**/*",
    "!package.json",
    "!CHANGELOG.md",
    "!README.md"
  ],
  "dependencies": {
    "bootstrap-sass": "~3.3.5",
    "svg4everybody": "~2.0.0"
  },
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "*/**/_bootstrap.scss",
        "*/**/bootstrap/*.scss"
      ],
      "normalize": {
        "js": "*/**/*.js",
        "css": [
          "*/**/*.css"
        ],
        "scss": [
          "*/**/*.scss"
        ]
      }
    }
  }
}