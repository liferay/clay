		</section>
	</div>

	<link rel="stylesheet" href="<%= rootPath %>/scss/highlightjs/github.css" />

	<script src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/8.4/highlight.min.js"></script>
	<script src="<%= rootPath %>/js/site/toggle_css.js"></script>

	<script>
		$(function() {
			var headerTopBar = $('#headerTopBar');
			var wrapper = $('#wrapper');

			var forceReflow = function(el) {
				el.css('display', 'table');

				setTimeout(function() {
					el.css('display', '');
				}, 0);
			};

			headerTopBar.find('.navbar-toggle').on(
				'click',
				function(event) {
					var sidenavContainer = headerTopBar.next('.sidenav-container');

					event.preventDefault();

					sidenavContainer.one(
						'webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend',
						function(event) {
							sidenavContainer.removeClass('sidenav-transition');

							forceReflow($('.main-nav-open'));
						}
					);

					sidenavContainer.toggleClass('closed').addClass('sidenav-transition');
					wrapper.toggleClass('main-nav-open');
				}
			);

			$('.side-navigation').on(
				'click',
				'li.side-navigation-dropdown-container > a',
				function(event) {
					event.preventDefault();

					$(this).nextAll('.collapse').toggleClass('in');
				}
			);
		});

		hljs.initHighlightingOnLoad();
	</script>
	</body>
</html>