{
	"browserslist": [
		"extends browserslist-config-clay"
	],
	"dependencies": {
		"@clayui/button": "*",
		"@clayui/data-provider": "*",
		"@clayui/form": "*",
		"@clayui/icon": "*",
		"@clayui/label": "*",
		"@clayui/layout": "*",
		"@clayui/link": "*",
		"@clayui/loading-indicator": "*",
		"@clayui/modal": "*",
		"@clayui/provider": "*",
		"@clayui/shared": "*",
		"@clayui/table": "*",
		"@clayui/tooltip": "*",
		"@tanstack/react-virtual": "3.0.0-beta.54",
		"aria-hidden": "^1.2.2",
		"classnames": "2.3.1",
		"fuzzy": "^0.1.3",
		"react-dnd": "11.1.1",
		"react-dnd-html5-backend": "11.1.1",
		"react-transition-group": "^4.4.1",
		"warning": "^4.0.3"
	},
	"description": "Clay UI components in React",
	"files": [
		"lib"
	],
	"keywords": [
		"clay",
		"react"
	],
	"license": "BSD-3-Clause",
	"main": "src/index.ts",
	"name": "@clayui/core",
	"peerDependencies": {
		"@clayui/css": "*",
		"react": "^16.0.0 || ^17.0.0 || ^18.0.0",
		"react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0"
	},
	"repository": "https://github.com/liferay/clay",
	"scripts": {
		"build": "yarn build:cjs && yarn build:esm",
		"build:cjs": "cross-env NODE_ENV=production babel src --root-mode upward --out-dir lib/cjs --extensions .ts,.tsx",
		"build:esm": "cross-env NODE_ENV=production babel src --root-mode upward --out-dir lib/esm --extensions .ts,.tsx --env-name esm",
		"buildTypes": "cross-env NODE_ENV=production tsc --project ./tsconfig.declarations.json",
		"test": "jest --config ../../jest.config.js"
	},
	"version": "3.156.0"
}
