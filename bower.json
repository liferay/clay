{
  "name": "lexicon",
  "main": "index.js",
  "homepage": "https://github.com/natecavanaugh/lexicon",
  "authors": [
    "Nate Cavanaugh",
    "Patrick Yeo"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap-sass": "~3.3.5",
    "svg4everybody": "~2.0.0"
  },
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "*/**/_bootstrap.scss",
        "*/**/bootstrap/*.scss"
      ],
      "normalize": {
        "js": "*/**/*.js",
        "css": [
          "*/**/*.css"
        ],
        "scss": [
          "*/**/*.scss"
        ]
      }
    }
  }
}
